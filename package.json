{
  "name": "ogess_frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npx http-server ./dist",
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "apexcharts": "^3.44.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.3.1",
    "datatables.net-bs5": "^1.13.6",
    "datatables.net-buttons-bs5": "^2.4.1",
    "datatables.net-dt": "^1.13.6",
    "datatables.net-responsive-bs4": "^2.5.0",
    "datatables.net-responsive-bs5": "^2.5.0",
    "datatables.net-vue3": "^2.1.3",
    "date-fns": "^2.30.0",
    "element-plus": "^2.3.9",
    "jquery": "^3.7.1",
    "moment": "^2.29.4",
    "pinia": "^2.1.6",
    "sweetalert2": "^11.9.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue3-apexcharts": "^1.4.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/bcryptjs": "^2.4.5",
    "@types/bootstrap": "^5.2.8",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^18.17.17",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "cypress": "^13.2.0",
    "eslint": "^8.49.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "npm-run-all2": "^6.0.6",
    "prettier": "^3.0.3",
    "sass": "^1.69.3",
    "start-server-and-test": "^2.0.0",
    "typescript": "~5.2.0",
    "vite": "^4.4.9",
    "vitest": "^0.34.4",
    "vue-tsc": "^1.8.11"
  }
}
